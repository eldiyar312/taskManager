{
  "name": "be-api",
  "version": "0.0.1",
  "description": "Backend",
  "private": true,
  "scripts": {
    "postinstall": "node ./builder.js",
    "build": "tsc -b",
    "build:po": "po-extract --outExt=.ts --outDir=./src/i18n/dictionaries --fixPo",
    "watch": "tsc-watch --onSuccess \"cross-env NODE_ENV=development yarn run start\"",
    "watch:po": "po-extract --outExt=.ts --outDir=./src/i18n/dictionaries --watch --fixPo",
    "start": "cross-env NODE_PATH=build/ node ./build",
    "db:migrate": "migrate-mongo",
    "lint": "eslint . --ext .ts",
    "test": "cross-env NODE_ENV=test node test-ci.js"
  },
  "dependencies": {
    "@fastify/cookie": "^7.4.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/helmet": "^9.1.0",
    "@fastify/multipart": "^7.7.3",
    "@fastify/passport": "^2.2.0",
    "@fastify/session": "^9.0.0",
    "@fastify/swagger": "^8.3.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@fastify/websocket": "^7.1.3",
    "@sendgrid/mail": "^7.7.0",
    "@shelf/jest-mongodb": "^4.1.6",
    "@types/dotenv-flow": "^3.2.0",
    "@types/jest": "^29.2.6",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^18.11.18",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "connect-mongodb-session": "^3.1.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fastify": "^4.11.0",
    "fastify-extract-definitions": "^3.0.1",
    "intl-messageformat": "^10.2.5",
    "jest": "^29.3.1",
    "mailgun.js": "^8.0.6",
    "migrate-mongo": "^9.0.0",
    "mongoose": "^6.8.4",
    "passport-local": "^1.0.0",
    "po-extract": "^1.1.1",
    "rollbar": "^2.26.1",
    "shared": "*",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "main": "./src"
}
